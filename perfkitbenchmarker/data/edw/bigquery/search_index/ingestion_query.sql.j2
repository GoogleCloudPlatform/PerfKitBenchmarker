LOAD DATA INTO {{table_name}}
(
    event_timestamp TIMESTAMP,
    insert_id STRING,
    hostname STRING,
    process_id INT64,
    parent_process_id INT64,
    user_name STRING,
    command_line STRING,
    file_path STRING,
    ip_address STRING,
    raw_log STRING
)
FROM FILES (
  format = 'CSV',
  uris = ['{{ storage_path }}/*.csv'],
  skip_leading_rows = 1
  );
