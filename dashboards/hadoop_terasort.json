              },
              "custom_query": true,
              "query": "SELECT\n\t'',\n\tREGEXP_EXTRACT(labels, r'\\|machine_type:(.*?)\\|') AS machine_type,\n\tREGEXP_EXTRACT(labels, r'\\|cloud:(.*?)\\|') AS cloud,\n\t%%EXPRESSION%% AS number\nFROM\n\t[{{ dataset_name }}.{{ table_name }}]\nWHERE\n\ttimestamp >= TIMESTAMP_TO_SEC(DATE_ADD(CURRENT_TIMESTAMP(), -3, 'MONTH')) AND\n\tproduct_name = 'PerfKitBenchmarker' AND\n\ttest = 'hadoop_terasort' AND\n\tmetric = 'Terasort Throughput Per Core' AND\n    REGEXP_MATCH(labels, r'\\|scratch_disk_size:%%DISK_SIZE%%\\|') AND\n    REGEXP_MATCH(labels, r'\\|scratch_disk_type:%%DISK_TYPE%%\\|') AND\n    REGEXP_MATCH(labels, r'\\|num_rows:%%NUM_ROWS%%\\|')\nGROUP BY\n\tmachine_type,\n\tcloud\nORDER BY\n  number ASC;",
              "queryError": null,
              "query_exec": "SELECT\n\t'',\n\tREGEXP_EXTRACT(labels, r'\\|machine_type:(.*?)\\|') AS machine_type,\n\tREGEXP_EXTRACT(labels, r'\\|cloud:(.*?)\\|') AS cloud,\n\tNTH(50, QUANTILES(value)) AS number\nFROM\n\t[{{ dataset_name }}.{{ table_name }}]\nWHERE\n\ttimestamp >= TIMESTAMP_TO_SEC(DATE_ADD(CURRENT_TIMESTAMP(), -3, 'MONTH')) AND\n\tproduct_name = 'PerfKitBenchmarker' AND\n\ttest = 'hadoop_terasort' AND\n\tmetric = 'Terasort Throughput Per Core' AND\n    REGEXP_MATCH(labels, r'\\|scratch_disk_size:500\\|') AND\n    REGEXP_MATCH(labels, r'\\|scratch_disk_type:standard\\|') AND\n    REGEXP_MATCH(labels, r'\\|num_rows:100000000\\|')\nGROUP BY\n\tmachine_type,\n\tcloud\nORDER BY\n  number ASC;",
